{
  "name": "user-consents",
  "version": "1.0.0",
  "description": "Project for keeping user consents",
  "author": "Damian Pasek <damianpasek@gmail.com>",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "tsc --outDir dist",
    "dev": "tsc && concurrently -r \"nodemon dist/index.js\" \"tsc -w --project tsconfig.json\"",
    "lint": "eslint src --ext ts,js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:create": "sequelize migration:generate",
    "test": "TZ=UTC jest ."
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
